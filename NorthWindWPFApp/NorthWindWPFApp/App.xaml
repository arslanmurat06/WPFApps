<Application x:Class="NorthWindWPFApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NorthWindWPFApp"
             
              xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"            
             StartupUri="MainWindow.xaml">
    <Application.Resources>

       

        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"  xmlns:vm="clr-namespace:NorthWindWPFApp.ViewModels"/>
            <FontFamily x:Key="AwesomeSolid">/Fonts/FontAwesomeSolid.ttf#Font Awesome 5 Free Solid</FontFamily>
            <FontFamily x:Key="AwesomeRegular">/Fonts/FontAwesomeRegular.ttf#Font Awesome 5 Free Regular</FontFamily>
            <FontFamily x:Key="AwesomeBrands">/Fonts/FontAwesomeBrands.ttf#Font Awesome 5 Brands</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme BaseTheme="Light" PrimaryColor="DeepPurple" SecondaryColor="Lime" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>


    </Application.Resources>
</Application>
